<section class="view-itinerary">
  <h3>View Itinerary</h3>
  <div class="itinerary">
    <h4><%= @itinerary.title %></h4>
    <img src="<%= asset_path("airplane-img.jpg")%>">
    <p><%= @itinerary.description %></p>
    <p>Budget: $<%= @itinerary.budget %></p>
    <p>Created by: <%= @itinerary.user.first_name %></p>
    <%= button_to [@itinerary.reviews.count, "Likes"], itinerary_reviews_path(@itinerary), method: :post, onclick: "likeButton(this)", class: "button-like" %>
  </div>
</section>
