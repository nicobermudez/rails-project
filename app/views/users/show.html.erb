<div class="show-page">
  <h3> My Itineraries</h3>
    <% if @user.itineraries %>
      <section class="my-itineraries">
        <p>You currently haven't created any itineraries</p>
        <div class="button"><a class= "btn-primary" href="<%= new_itinerary_path %>">Create New</a></div>
      </section>
    <% else %>
      <section class="my-itineraries">
      <%= @user.itineraries.each do |itinerary| %>
        <div class="itinerary">

        </div>
      <% end %>
      </section>
    <% end %>

  <h3> My Library </h3>
    <!-- Liked Itineraries -->
</div>
